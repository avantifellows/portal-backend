# Portal Backend Environment Variables
# Copy this file to .env and fill in the actual values

# JWT Configuration
JWT_SECRET_KEY=your-secret-key-here-use-different-for-prod-and-staging

# Database Service Configuration
DB_SERVICE_URL=http://localhost:8000
DB_SERVICE_TOKEN=your-db-service-token-here

# Business Logic Configuration
DEFAULT_ACADEMIC_YEAR=2025-2026

# AWS SQS Configuration
SQS_ACCESS_KEY=your-sqs-access-key
SQS_SECRET_ACCESS_KEY=your-sqs-secret-key
AWS_SQS_URL=https://sqs.region.amazonaws.com/account/queue-name
